<style type='text/css'>
#background-images li {
    list-style-type: none;
    padding: 0;
    margin: 0;
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    background-repeat: no-repeat;
    background-size: cover;
    background-position: 50%;
    z-index: -1;
}
#shape {

}
</style>

<ul id='background-images'>
  <li id='0'></li>
  <li id='1'></li>
  <li id='2'></li>
  <li id='3'></li>
  <li id='4'></li>
  <li id='5'></li>
  <li id='6'></li>
  <li id='7'></li>
</ul>

<div id='shape'></div>

<script type='text/javascript'>
// BEGIN: query google image search and set background-images
queryGoogleForPictures('south pagoda', Math.floor(Math.random()*92));
function queryGoogleForPictures(search, position) {
  $.ajax({
    url: 'https://ajax.googleapis.com/ajax/services/search/images',
    dataType: 'jsonp',
    data: {
      v: '1.0',
      rsz: 8,
      start: position,
      q: search
    },
    success: function(response) {
      if (response.responseData == null) {
        console.log('no results');
      } else {
        for (i = 0; i < 8; i++) {
          $("#"+i).css('background-image', 'url('+response.responseData.results[i].unescapedUrl+')');
        }
      }
    }
  });
}
// END: query google image search
</script>