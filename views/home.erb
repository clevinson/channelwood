<ul id='the-list'>
  <% @releases.each do |release| %>
    <li>
      <div class='art'>
        <img src='<%= release.cover_art %>' alt='art'>
      </div>
      <div class='info'>
        <h2 class='artist'><%= release.artist %></h2>
        <h2 class='title'><%= release.title %></h2>
        <p><%= release.cat_no %>
        <time><%= release.release_date %></time>
        <a href="/digital/<%= release.cat_no %>">digital</a> / <a href="/physical/<%= release.cat_no %>">physical</a></p>
      </div>
    </li>
  <% end %>
