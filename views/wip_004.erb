  <script type="text/paperscript" canvas="myCanvas">

var margin = 200;

var bg = new Path.Rectangle(new Point(0, 0), view.size);
bg.fillColor = '#0000cc';

var Star = function(point) {
  this.point = point;
  this.vector = this.point - new Point(view.size.width/2, view.size.height/2);
  this.size = Math.abs(this.vector.length/10);

  this.path = new Path.Rectangle(this.point, this.size);
  this.path.strokeColor = 'white';
}
Star.prototype.iterate = function() {
  this.path.position += this.vector/80;
  this.vector = this.path.position - new Point(view.size.width/2, view.size.height/2);
  this.path.scale(1.008);

  if (this.path.position.x > view.size.width+margin || this.path.position.x < 0-margin || this.path.position.y < 0-margin || this.path.position.y > view.size.height+margin) {
    this.reset(getRandomPoint());
  }
}
Star.prototype.reset = function(point) {
  this.path.remove();

  this.point = point;
  this.vector = this.point - new Point(view.size.width/2, view.size.height/2);
  this.size = Math.abs(this.vector.length/10);

  this.path = new Path.Rectangle(this.point, this.size);
  this.path.strokeColor = 'white';
}

//var s = new Star(getRandomPoint());
//
//function onFrame() {
//  s.iterate();
//}

function getRandomPoint() {
  var width = view.size.width/2;
  var height = view.size.height/2;
  var x = Math.random()*width+view.size.width/4;
  var y = Math.random()*height+view.size.height/4;
  return new Point(x, y);
};

var stars = [];
for (var i = 0; i < 10; i++) {
  var star = new Star(getRandomPoint());
  stars.push(star);
}
function onFrame() {
  for (i = 0; i < stars.length; i++){
    stars[i].iterate();
  }

}

